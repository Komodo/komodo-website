        <%- @db.promotions().pricing.description %>
    </div>
</div>
<% end %>

<div class="outer <%= 'promotions-enabled' if @db.promotions().enterprise.enabled or @db.promotions().personal.enabled %>">
<div class="inner">

    <span class="currency">
        All prices are in US dollars
    </span>
    
    <span class="testimonials">
        On the fence? Check out our
        <a href="http://www.activestate.com/company/testimonials#block-views-testimonials-block_8">
            Testimonials!
        </a>
    </span>

    <div class="tabs">
    <ul>
        <li><a href="#ide">Komodo IDE</a></li>
        <li><a href="#bundles">Bundles</a></li>
    </ul>
    <div class="clearfix"></div>

    <div id="ide">
        <ul class="block">
            <li class="enterprise">
                <h2>For Companies</h2>

                <a href="https://store.activestate.com/cart/add/p2_q1-iupsell_yes?destination=cart"
                        data-analytics="buy:komodo:single">
                    <figure>$295</figure>
                </a>

                <p>
                    Companies & Organizations
                </p>
                <p>
                    <a href="/pricing/subscriptions"
                       data-modal data-modal-elem="#subscription-info"
                       data-analytics="buy:komodo:subcribe_info">
